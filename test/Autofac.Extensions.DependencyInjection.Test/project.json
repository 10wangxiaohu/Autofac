{
  "buildOptions": {
    "additionalArguments": [
      "/ruleset:../../build/Analyzers.ruleset"
    ],
    "embed": {
      "include": [
        "**/*.resx",
        "Files/*.*"
      ]
    },
    "keyFile": "../../Autofac.snk"
  },
  "dependencies": {
    "Autofac": "4.0.0-rc2-*",
    "Autofac.Extensions.DependencyInjection": "4.0.0-rc2-*",
    "Microsoft.Extensions.DependencyInjection": "1.0.0-rc2-*",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0-rc2-*",
    "Microsoft.Extensions.DependencyInjection.Specification.Tests": "1.0.0-rc2-*",
    "Microsoft.Extensions.Options": "1.0.0-rc2-*",
    "Microsoft.NETCore.Platforms": "1.0.1-rc2-*",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "dotnet-test-xunit": "1.0.0-rc3-*",
    "xunit": "2.2.0-beta2-*"
  },
  "frameworks": {
    "net46": {}
  },
  "packOptions": {
    "owners": [
      "Autofac Contributors"
    ]
  },
  "testRunner": "xunit"
}