{
  "buildOptions": {
    "additionalArguments": [
      "/ruleset:../../build/Analyzers.ruleset"
    ],
    "embed": {
      "include": [
        "**/*.resx",
        "Files/*.*"
      ]
    },
    "keyFile": "../../Autofac.snk"
  },
  "dependencies": {
    "Autofac": "4.0.0-rc2-*",
    "Autofac.Test.Scenarios.ScannedAssembly": "1.0.0-*",
    "Microsoft.NETCore.Platforms": "1.0.1-rc2-*",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "dotnet-test-xunit": "1.0.0-rc3-*",
    "xunit": "2.2.0-beta2-*"
  },
  "frameworks": {
    "net46": {
      "frameworkAssemblies": {
        "System.ComponentModel.Composition": "4.0.0.0",
        "System.Data": "4.0.0.0"
      }
    }
  },
  "packOptions": {
    "authors": [
      "Autofac Contributors"
    ]
  },
  "testRunner": "xunit"
}