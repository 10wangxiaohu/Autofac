{
  "version": "4.0.0-rc2-*",
  "description": "Autofac is an IoC container for Microsoft .NET. It manages the dependencies between classes so that applications stay easy to change as they grow in size and complexity.",
  "packOptions": {
    "summary": "Core assembly for the Autofac Inversion of Control container",
    "tags": [ "autofac", "di", "ioc", "dependencyinjection" ],
    "owners": [ "Autofac Contributors" ],
    "iconUrl": "https://cloud.githubusercontent.com/assets/1156571/13684110/16b8f152-e6bf-11e5-84ae-22c66c6d351a.png",
    "projectUrl": "http://autofac.org",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/autofac/Autofac"
    }
  },
  "buildOptions": {
    "keyFile": "../../Autofac.snk",
    "nowarn": [
      "CS1591"
    ],
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "frameworks": {
    "netstandard1.1": {
      "dependencies": {
        "System.Collections.Concurrent": "4.0.12-*",
        "System.ComponentModel": "4.0.1-*",
        "System.Diagnostics.Debug": "4.0.11-*",
        "System.Diagnostics.Tools": "4.0.1-*",
        "System.Globalization": "4.0.11-*",
        "System.Linq": "4.1.0-*",
        "System.Linq.Expressions": "4.0.11-*",
        "System.Reflection": "4.1.0-*",
        "System.Reflection.Extensions": "4.0.1-*",
        "System.Reflection.Primitives": "4.0.1-*",
        "System.Resources.ResourceManager": "4.0.1-*",
        "System.Runtime": "4.1.0-*",
        "System.Runtime.Extensions": "4.1.0-*",
        "System.Threading": "4.0.11-*"
      },
      "imports": [
        "dotnet5.2"
      ]
    }
  }
}