{
  "version": "1.0.0-*",
  "buildOptions": {
    "compile": {
      "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components"
      ]
    }
  },
  "publishOptions": {
    "exclude": [
      "node_modules",
      "bower_components",
      "**.xproj",
      "**.user",
      "**.vspscc"
    ]
  },
  "dependencies": {
    "Microsoft.AspNetCore.Mvc": "1.0.0-rc2-*",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-rc2-*",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-*",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-rc2-*",
    "Microsoft.NETCore.Platforms": "1.0.1-rc2-*",
    "Autofac": "4.0.0-rc2-*",
    "Autofac.Extensions.DependencyInjection": "4.0.0-rc2-*"
  },
  "commands": {
    "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls http://localhost:5000"
  },
  "frameworks": {
    "net451": { },
    "netstandardapp1.5": {
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-*"
      },
      "imports": "dnxcore50"
    }
  },
  "webroot": "wwwroot"
}
